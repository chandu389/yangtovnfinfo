  <grouping name="notif-vnf">
    <leaf name="type">
      <description>
        <text>Network Function Type</text>
      </description>
      <type name="network-function-type"/>
    </leaf>
    <container name="esc">
      <description>
        <text>ESC managing this Network Function.</text>
      </description>
      <leaf name="ip">
        <type name="inet:ip-address"/>
      </leaf>
      <leaf name="name">
        <type name="identifier"/>
      </leaf>
    </container>
    <container name="location">
      <leaf name="data-center">
        <description>
          <text>Data Center this Network Function is deployed at.</text>
        </description>
        <type name="identifier"/>
      </leaf>
      <leaf name="pod">
        <description>
          <text>POD this Network Function is deployed at.</text>
        </description>
        <type name="identifier"/>
      </leaf>
    </container>
  </grouping>
  <grouping name="notif-vm">
    <leaf name="type">
      <description>
        <text>VDU type as defined in this Network Function's VNFD.</text>
